<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<TextField fx:id="login" layoutX="490.0" layoutY="320.0"
			prefHeight="25.0" prefWidth="300.0" promptText="Login" />
		<TextField fx:id="password" layoutX="490.0" layoutY="360.0"
			prefHeight="25.0" prefWidth="300.0" promptText="Password" />

		<Button layoutX="600.0" layoutY="400.0" mnemonicParsing="false"
			onMouseClicked="#submit" prefWidth="80.0" text="Button" />
	</children>
</AnchorPane>
