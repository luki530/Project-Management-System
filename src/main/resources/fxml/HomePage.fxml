<?xml version="1.0" encoding="UTF-8"?>


<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<ListView fx:id="projectList" layoutX="50.0" layoutY="50.0"
			onMouseClicked="#projectClicked" prefHeight="620.0" prefWidth="1100.0" />
		<Button layoutX="1177.0" layoutY="632.0" mnemonicParsing="false"
			prefHeight="25.0" prefWidth="80.0" text="Users" />
		<Button fx:id="newProject" layoutX="1177.0" layoutY="63.0"
			mnemonicParsing="false" onAction="#newProject" onMouseClicked="#newProject"
			prefHeight="25.0" prefWidth="80.0" text="New Project" />
		<Button fx:id="sort" layoutX="1180.0" layoutY="99.0"
			mnemonicParsing="false" onAction="#sort" onMouseClicked="#sort"
			prefHeight="25.0" prefWidth="80.0" text="Sort" />
		<ChoiceBox fx:id="sortBy" layoutX="1177.0" layoutY="140.0"
			prefHeight="25.0" prefWidth="80.0" />
	</children>
</AnchorPane>
